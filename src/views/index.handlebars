  <div class="row justify-content-center text-center">
        <div class="h1" id="title1">Create your resume digitally</div>
    </div><hr class="border-5"><br>
    <div class="row justify-content-center text-center">
        <div class="col-md-6">
          <a href="/api/cv" class="btn btn-light form-control">START</a>
        </div>
    </div><br><br><br>
    <div class="row justify-content-center text-center">
        <div class="col-md-6">
          
          {{#each data}}
            <div class="card" style="width: 18rem;">
                <img src="/img/{{nameImg}}" class="card-img-top" alt="...">
                <div class="card-body">
                    
                        <h5 class="card-title">{{name}}</h5>
                        <p class="card-text">{{occupation}}</p>
                        <a href="/wiewCV/" class="btn btn-success form-control">Go Views</a>
                    </div>
                  </div><br><br><br>
           {{/each}}
        </div>
    </div>